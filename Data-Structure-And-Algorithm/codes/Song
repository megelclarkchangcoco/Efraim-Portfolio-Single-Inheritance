import java.util.LinkedList;
import java.util.Scanner;

// Create a class to represent a Song
class Song {
    private String name;
    private int year;

    // Constructor to initialize the name and year of the Song
    public Song(String name, int year) {
        this.name = name;
        this.year = year;
    }

    // Getter method to retrieve the name of the Song
    public String getName() {
        return name;
    }

    // Getter method to retrieve the year of the Song
    public int getYear() {
        return year;
    }
}
